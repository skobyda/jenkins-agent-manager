<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">
    <f:entry title="Select desired build result(s)">
        <f:checkbox title="${%Success}" field="success" />
        <f:checkbox title="${%Failure}" field="failure" />
        <f:checkbox title="${%Aborted}" field="aborted" />
        <f:checkbox title="${%Unstable}" field="unstable" />
        <f:checkbox title="${%Not build}" field="notBuilt" />
    </f:entry>
</j:jelly>
